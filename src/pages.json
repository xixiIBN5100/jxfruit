{
	// 组件自动引入规则
	"easycom": {
		// 是否开启自动扫描
		"autoscan": true,
		// 以正则方式自定义组件匹配规则
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			// 以 Xtx 开头的组件，在 components 文件夹中查找引入（需要重启服务器）
			"^Xtx(.*)": "@/components/Xtx$1.vue"
		}
	},
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/suggestion/suggestion",
			"style": {
				"navigationBarTitleText": "意见箱"
			}
		},
		{
			"path": "pages/category/category",
			"style": {
				"navigationBarTitleText": "商品分类"
			}
		},
		{
			"path": "pages/search-result/search-result",
			"style": {
				"navigationBarTitleText": "搜索结果"
			}
		},
		{
			"path": "pages/cart/cart",
			"style": {
				"navigationBarTitleText": "购物车"
			}
		},
		{
			"path": "pages/cart/cart2",
			"style": {
				"navigationBarTitleText": "购物车"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/hot/hot",
			"style": {
				"navigationBarTitleText": "热门推荐"
			}
		},
		{
			"path": "pages/goods/goods",
			"style": {
				"navigationBarTitleText": "商品详情"
			}
		},
		{
			"path": "pages/coming-soon/coming-soon",
			"style": {
				"navigationBarTitleText": "敬请期待"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	// 设置 TabBar
	"tabBar": {
		"color": "black",
		"selectedColor": "#FFDB8E",
		"backgroundColor": "white",
		"borderStyle": "white",
		"list": [
			{
				"text": "首页",
				"pagePath": "pages/index/index",
				"iconPath": "static/tabs/home_default.png",
				"selectedIconPath": "static/tabs/home_selected.png"
			},
			{
				"text": "分类",
				"pagePath": "pages/category/category",
				"iconPath": "static/tabs/category_default.png",
				"selectedIconPath": "static/tabs/category_selected.png"
			},
			{
				"text": "购物车",
				"pagePath": "pages/cart/cart",
				"iconPath": "static/tabs/cart_default.png",
				"selectedIconPath": "static/tabs/cart_selected.png"
			},
			{
				"text": "我的",
				"pagePath": "pages/my/my",
				"iconPath": "static/tabs/user_default.png",
				"selectedIconPath": "static/tabs/user_selected.png"
			}
		]
	},
	// 分包加载规则
	"subPackages": [
		{
			"root": "pagesAdmin",
			"pages": [
				{
					"path": "goodsManage/goodsManage",
					"style": {
						"navigationBarTitleText": "商品管理页面"
					}
				},
				{
					"path": "goodsDetails/goodsDetails",
					"style": {
						"navigationBarTitleText": "商品详情管理页面"
					}
				},
				{
					"path": "orderManage/orderManage",
					"style": {
						"navigationBarTitleText": "订单管理页面"
					}
				},
				{
					"path": "orderDetails/orderDetails",
					"style": {
						"navigationBarTitleText": "订单详情管理页面"
					}
				},
          {
            "path": "couponManage/couponManage",
            "style": {
              "navigationBarTitleText": "优惠券管理页面",
              "navigationBarTextStyle":"black",
              "navigationBarBackgroundColor":"#F8F8F8"
            }
          },
          {
            "path": "couponDetails/couponDetails",
            "style": {
              "navigationBarTitleText": "优惠券详情管理页面"
            }
          },
          {
            "path": "qrCodeVerify/qrCodeVerify",
            "style": {
              "navigationBarTitleText": "购物券管理页面"
            }
          },
          {
            "path": "newGoods/newGoods",
            "style": {
              "navigationBarTitleText": "新商品创建页面"
            }
          }
			]
		},
		{
			// 子包的根目录
			"root": "pagesMember",
			// 页面路径和窗口表现
			"pages": [
				{
					"path": "settings/settings",
					"style": {
						"navigationBarTitleText": "设置"
					}
				},
				{
					"path": "profile/profile",
					"style": {
						"navigationStyle": "custom",
						"navigationBarTextStyle": "white",
						"navigationBarTitleText": "个人信息"
					}
				},
				{
					"path": "address/address",
					"style": {
						"navigationBarTitleText": "地址管理"
					}
				},
				{
					"path": "address-form/address-form",
					"style": {
						"navigationBarTitleText": "我的地址"
					}
				},
				{
					"path": "priviledge/priviledge",
					"style": {
						"navigationBarTitleText": "我的权益"
					}
				},
				{
					"path": "coupon/coupon",
					"style": {
						"navigationBarTitleText": "优惠券"
					}
				},
				{
					"path": "member-center/member-center",
					"style": {
						"navigationBarTitleText": "会员中心"
					}
				},
        {
          "path": "couponCode/couponCode",
          "style": {
            "navigationBarTitleText": "券码页面"
          }
        }
			]
		},
		{
			"root": "pagesOrder",
			"pages": [
				{
					"path": "create/create",
					"style": {
						"navigationBarTitleText": "填写订单"
					}
				},
				{
					"path": "detail/detail",
					"style": {
						"navigationBarTitleText": "订单详情",
						"navigationStyle": "custom"
					}
				},
				{
					"path": "payment/payment",
					"style": {
						"navigationBarTitleText": "支付结果"
					}
				},
				{
					"path": "list/list",
					"style": {
						"navigationBarTitleText": "订单列表"
					}
				},
				{
					"path": "comment/comment",
					"style": {
						"navigationBarTitleText": "评论页面"
					}
				},
				{
					"path": "refund/refund",
					"style": {
						"navigationBarTitleText": "退款"
					}
				}
			]
		}
	],
	// 分包预下载规则
	"preloadRule": {
		"pages/my/my": {
			"network": "all",
			"packages": [
				"pagesMember"
			]
		}
	}
}
